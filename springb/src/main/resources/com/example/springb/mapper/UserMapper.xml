<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.springb.mapper.UserMapper">
    <select id="userNum" resultType="int">
        select count(userNum) as uNum from user;
    </select>
    <select id="findAll" resultType="com.example.springb.domain.UserVo">
        SELECT * FROM user;
    </select>
</mapper>